#include <iostream>
using namespace std;

int main() {
    // Write C++ code here
    long long int n,x,p=1;
    int counter=0;
    cout<<"Enter a number:\n";
    cin>>n;
    for( int i=0;i<n;i++)
    {
        cin>>x;
        while(x%10==0)
        {
            counter++;
            x=x/10;
        }
        p=p*x;
        
    }
    cout<<p;
    for(int i=0;i<counter;i++)
    {
        cout<<0;
    }
    

    return 0;
}
